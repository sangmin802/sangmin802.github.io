\n  methods : {\n    enter(){\n      alert('enter')\n    },\n    pageDown(){\n      alert('pageDown')\n    },\n    alt67(){\n      alert('alt67')\n    },\n    withCtrl(){\n      alert('withCtrl');\n    },\n    onleCtrl(){\n      alert('onlyCtrl');\n    },\n    nokey(){\n      alert('nokey');\n    }\n  }\n})\n\nnew Vue({\n  el : '#event6',\n  delimiters : ['<%', '%>'],\n  methods : {\n    left(){\n      alert('left')\n    },\n    right(){\n      alert('right')\n    },\n    middle(){\n      alert('middle')\n    }\n  }\n})\n</script>","frontmatter":{"title":"Vue 이벤트 처리","date":"September 07, 2020"}}},"pageContext":{"slug":"/Study/Vue/Vue_08_events/","previous":{"fields":{"slug":"/Study/Vue/Vue_07_v-for/"},"frontmatter":{"title":"Vue 리스트 렌더링","category":"Study","draft":false,"tag":"Vue.js"}},"next":{"fields":{"slug":"/Study/Vue/Vue_09_form/"},"frontmatter":{"title":"Vue 양식 입력 바인딩","category":"Study","draft":false,"tag":"Vue.js"}}}},"staticQueryHashes":["3128451518","3665928227"]}