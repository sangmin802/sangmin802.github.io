<span class=\"token operator\">:</span>layout_height<span class=\"token operator\">=</span><span class=\"token string\">\"50dp\"</span>\n            android<span class=\"token operator\">:</span>layout_marginStart<span class=\"token operator\">=</span><span class=\"token string\">\"8dp\"</span>\n            android<span class=\"token operator\">:</span>layout_marginTop<span class=\"token operator\">=</span><span class=\"token string\">\"8dp\"</span>\n            app<span class=\"token operator\">:</span>layout_constraintStart_toStartOf<span class=\"token operator\">=</span><span class=\"token string\">\"parent\"</span>\n            app<span class=\"token operator\">:</span>layout_constraintTop_toTopOf<span class=\"token operator\">=</span><span class=\"token string\">\"parent\"</span>\n            android<span class=\"token operator\">:</span>src<span class=\"token operator\">=</span><span class=\"token string\">\"@drawable/ic_baseline_menu_64\"</span> <span class=\"token operator\">/</span><span class=\"token operator\">></span>\n\n    <span class=\"token operator\">&lt;</span><span class=\"token operator\">/</span>androidx<span class=\"token punctuation\">.</span>constraintlayout<span class=\"token punctuation\">.</span>widget<span class=\"token punctuation\">.</span>ConstraintLayout<span class=\"token operator\">></span>\n    <span class=\"token operator\">&lt;</span>com<span class=\"token punctuation\">.</span>google<span class=\"token punctuation\">.</span>android<span class=\"token punctuation\">.</span>material<span class=\"token punctuation\">.</span>navigation<span class=\"token punctuation\">.</span>NavigationView\n        android<span class=\"token operator\">:</span>id<span class=\"token operator\">=</span><span class=\"token string\">\"@+id/nav_view\"</span>\n        android<span class=\"token operator\">:</span>layout_width<span class=\"token operator\">=</span><span class=\"token string\">\"wrap_content\"</span>\n        android<span class=\"token ope